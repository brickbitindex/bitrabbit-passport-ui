{
  "name": "bitrabbit-passport-ui",
  "version": "0.4.9",
  "description": "BitRabbit Passport UI.",
  "main": "lib",
  "files": [
    "lib"
  ],
  "scripts": {
    "clear": "rm -rf dist && rm -rf dist_dev",
    "build": "rm -rf lib && babel src -d lib",
    "watch": "rm -rf lib && babel src -d lib -w",
    "build_test": "rm -rf test_dist && babel test -d test_dist",
    "test": "npm run build && npm run build_test && cp -r test/data test_dist/data && mocha test_dist/index.js --slow 5000 --timeout 10000",
    "deploy": "npm run clear && NODE_ENV=production webpack -p --display-error-details --config webpack/webpack.deploy.config.js --profile --json > webpack-stats.json && scp -r dist/* deploy@39.98.92.83:~/public/g/bpc/",
    "dev": "npm run clear && webpack -p --display-error-details --config webpack/webpack.dev.config.js -w",
    "dev_html": "http-server . -p 8080",
    "server": "http-server ./dist_dev -p 9090"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brickbitindex/bitrabbit-passport-ui.git"
  },
  "keywords": [
    "forum",
    "plugin"
  ],
  "bugs": {
    "url": "https://gitlab.com/brickworks/bitrabbit/forum-jsbridge/issues"
  },
  "homepage": "https://gitlab.com/brickworks/bitrabbit/forum-jsbridge#readme",
  "author": "sekaiamber",
  "license": "MIT",
